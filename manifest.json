{
    "name": "Dialogflow Helper",
    "version": "0.1",
    "description": "Dialogflow Helper",
    "content_scripts": [
        {
          "matches": ["https://console.dialogflow.com/api-client/demo/embedded/*"],
          "js": ["contentScript.js"],
          "run_at": "document_end"
        }
      ],
    "web_accessible_resources": ["injectScript.js"],
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "manifest_version": 2
  }